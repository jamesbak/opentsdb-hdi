{
    "name": "default",
    "description": "Default theme for OpenTSDB service",
    "configuration": {
        "layouts": [
            {
                "name": "default",
                "tabs": [
                    {
                        "name": "settings",
                        "display-name": "Settings",
                        "layout": {
                            "tab-columns": "3",
                            "tab-rows": "2",
                            "sections": [
                                {
                                    "name": "section-opentsdb-core",
                                    "display-name": "Core",
                                    "row-index": "0",
                                    "column-index": "0",
                                    "row-span": "1",
                                    "column-span": "3",
                                    "section-columns": "3",
                                    "section-rows": "1",
                                    "subsections": [
                                        {
                                            "name": "subsection-opentsdb-core",
                                            "row-index": "0",
                                            "column-index": "0",
                                            "row-span": "1",
                                            "column-span": "3"
                                        }
                                    ]
                                },
                                {
                                    "name": "section-opentsdb-http",
                                    "display-name": "HTTP",
                                    "row-index": "1",
                                    "column-index": "0",
                                    "row-span": "1",
                                    "column-span": "3",
                                    "section-columns": "3",
                                    "section-rows": "1",
                                    "subsections": [
                                        {
                                            "name": "subsection-opentsdb-http",
                                            "row-index": "0",
                                            "column-index": "0",
                                            "row-span": "1",
                                            "column-span": "3"
                                        }
                                    ]
                                }
                            ]
                        }
                    }
                ]
            }
        ],
        "placement": {
            "configuration-layout": "default",
            "configs": [
                {
                    "config": "opentsdb-site/tsd.network.port",
                    "subsection-name": "subsection-opentsdb-core"
                },
                {
                    "config": "opentsdb-site/tsd.core.auto_create_metrics",
                    "subsection-name": "subsection-opentsdb-core"
                },
                {
                    "config": "opentsdb-site/tsd.http.staticroot",
                    "subsection-name": "subsection-opentsdb-http"
                },
                {
                    "config": "opentsdb-site/tsd.http.cachedir",
                    "subsection-name": "subsection-opentsdb-http"
                }
            ]
        },
        "widgets": [
            {
                "config": "opentsdb-site/tsd.core.auto_create_metrics",
                "widget": {
                    "type": "toggle"
                }
            },
            {
                "config": "opentsdb-site/tsd.http.staticroot",
                "widget": {
                    "type": "directory"
                }
            },
            {
                "config": "opentsdb-site/tsd.http.cachedir",
                "widget": {
                    "type": "directory"
                }
            }
        ]
    }
}